{
  "name": "Developers Developers DEVELOPERS",
  "version": "1.0.0",
  "description": "Steve Ballmer says one of his many DEVELOPERS each time you hover the word developer(s)",
  "manifest_version": 3,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "permissions": ["offscreen"],
  "author": { "email": "svante@swantzter.se" },
  "homepage_url": "https://github.com/swantzter/crx-developers-developers-developers"
}
